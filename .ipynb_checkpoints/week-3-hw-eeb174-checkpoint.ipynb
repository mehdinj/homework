    "#Question 3: Calculating AT exercise\n",
    "\n",
    "DNA = \"ACTGATCGATTACGTATAGTATTTGCTATCATACATATATATCGATGCGTTCAT\"\n",
    "length = len(DNA)\n",
    "\n",
    "A = DNA.count('A')\n",
    "T = DNA.count('T')\n",
    "\n",
    "\n",
    "AT = (A + T) / length\n",
    "\n",
    "\n",
    "print(\"AT content is \" + str(AT))"
